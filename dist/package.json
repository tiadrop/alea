{
  "name": "@xtia/alea",
  "version": "1.1.1",
  "description": "A practical randomness toolkit. Sample/shuffle arrays, generate strings, create UUIDs, and more, with a unified API for Math.random(), crypto, or custom/transformed PRNGs.",
  "repository": {
    "url": "https://github.com/tiadrop/alea",
    "type": "github"
  },
  "sideEffects": false,
  "types": "./index.d.ts",
  "main": "./index.js",
  "exports": {
    ".": {
      "types": "./entry/other.d.ts",
      "browser": "./entry/browser.js",
      "workerd": "./entry/browser.js",
      "deno": "./entry/browser.js",
      "node": "./entry/node.js",
      "bun": "./entry/node.js",
      "default": "./entry/other.js"
    },
    "./prng": {
      "types": "./entry/prng/index.d.ts",
      "default": "./entry/prng/index.js"
    },
    "./internal/*": null,
    "./entry/*": null
  },
  "scripts": {
    "prepublishOnly": "cp ../README.md .",
    "postpublish": "rm README.md"
  },
  "keywords": [
    "random",
    "rng",
    "crypto",
    "prng"
  ],
  "author": "Aleta Lovelace",
  "license": "MIT"
}
